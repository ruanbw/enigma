{
  "compilerOptions": {
    "lib": ["esnext"],
    "module": "Node16",
    "target": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "outDir": "./Dist",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": ["Sources/**/*"]
}
